package week1.day1;

public class PrimeNumber {

	public static void main(String[] args) 
	{
		
		int num1 = 18;
		boolean flag = true;
		for (int i=2;i<=num1/2; i++)
		{
			if(num1%i ==0)
			{
				System.out.println(num1 +"is not a prime number");
				flag = false;
				break;
			}
			
		}
		if (flag) // flag = true
		{
			System.out.println(num1 + "is a prime number");
		}
	}
}
